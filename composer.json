{
	"name": "listenwei/l4-migrations-generator",
	"description": "Generates Laravel4 Migrations from an existing database, From Xethron/migrations-generator",
	"keywords": ["laravel", "migration", "generator", "migrations", "artisan"],
	"license": "MIT",
	"authors": [
		{
			"name": "Zhu Fengwei",
			"email": "m15275049388@163.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"illuminate/support": "~4.1",
		"way/generators": "2.*",
		"doctrine/dbal": "~2.4"
	},
	"autoload": {
		"psr-0": {
			"Xethron\\MigrationsGenerator": "src/"
		}
	},
	"minimum-stability": "stable",
	"require-dev": {
		"phpunit/phpunit": ">=4.0.0",
		"mockery/mockery": ">=0.9.0",
		"illuminate/cache": ">=4.1.0",
		"illuminate/console": ">=4.1.0"
	}
}
